title = {{Graph-based comparison of executable objects (english version)}},
url = {http://actes.sstic.org/SSTIC05/Analyse{\_}differentielle{\_}de{\_}binaires/SSTIC05-article-Flake-Graph{\_}based{\_}comparison{\_}of{\_}Executable{\_}Objects.pdf},
year = {2005}
}
